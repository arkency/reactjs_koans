{
  "name": "reactjs_koans",
  "version": "0.0.1",
  "description": "React.js Koans",
  "main": "index.js",
  "scripts": {
    "compile": "jsx -x jsx --es6module exercises/es6 src/es6 > /dev/null 2>&1",
    "setup": "npm install && ncp koans/ exercises/",
    "start": "npm run compile > /dev/null 2>&1 && nodemon server.js",
    "test-es6": "npm run compile > /dev/null 2>&1 && mocha -b --compilers js:mocha-traceur -r test/es6/helpers.js test/es6/**/*.js & true",
    "watch": "onchange exercises/es6/*.jsx -- npm run test-es6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arkency/reactjs_koans.git"
  },
  "keywords": [
    "react.js",
    "kata",
    "tests"
  ],
  "author": "Arkency",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arkency/reactjs_koans/issues"
  },
  "homepage": "https://github.com/arkency/reactjs_koans",
  "devDependencies": {
    "babel-core": "^5.5.1",
    "babel-loader": "^5.1.3",
    "express": "^4.12.4",
    "jsdom": "^3.1.2",
    "lodash": "^3.8.0",
    "mocha": "^2.2.4",
    "mocha-traceur": "^2.1.0",
    "ncp": "^2.0.0",
    "nodemon": "^1.3.7",
    "onchange": "^1.1.0",
    "react": "^0.13.3",
    "react-hot-loader": "^1.2.3",
    "react-tools": "^0.13.3",
    "sinon": "^1.15.4",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
  }
}
